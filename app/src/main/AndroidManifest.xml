<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androide.cmrcet">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UploadMainActivity"></activity>
        <activity android:name=".WorkshopActivity" />
        <activity android:name=".GalleryMainActivity" />
        <activity android:name=".SignInMainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpMainActivity" />
        <activity
            android:name=".StudentsActivity"
            android:label="@string/title_activity_students"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>